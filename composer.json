{
    "name": "okipa/laravel-toggle-switch-button",
    "description": "A customizable toggle switch button generator for Laravel.",
    "keywords": [
        "okipa",
        "package",
        "php",
        "laravel",
        "toggle",
        "switch",
        "button",
        "generate",
        "generator",
        "generation",
        "html"
    ],
    "homepage": "https://github.com/Okipa/laravel-toggle-switch-button",
    "license": "MIT",
    "authors": [
        {
            "name": "Arthur LORENT",
            "email": "arthur.lorent@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Okipa\\LaravelToggleSwitchButton\\": "src/"
        },
        "files": [
            "src/Helpers/toggleSwitchButtonHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Okipa\\LaravelToggleSwitchButton\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Okipa\\LaravelToggleSwitchButton\\ToggleSwitchButtonServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
